{
  "private": true,
  "scripts": {
    "format": "elm-format src tests",
    "start": "elm-doc-preview",
    "review": "elm-review --fix src",
    "test": "elm-review src && elm-verify-examples --fail-on-warn -r && elm-format --validate src tests && elm make --docs=/tmp/docs.json"
  },
  "dependencies": {
    "elm": "^0.19.1-5"
  },
  "devDependencies": {
    "elm-doc-preview": "^5.0.5",
    "elm-format": "^0.8.5",
    "elm-review": "^2.5.5",
    "elm-verify-examples": "^5.0.0"
  }
}
